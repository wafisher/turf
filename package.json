  "name": "@turf/turf",
  "version": "4.7.3",
  "description": "a JavaScript library for performing geospatial operations with GeoJSON",
  "main": "main",
  "module": "index",
  "jsnext:main": "index",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "main.js",
    "turf.js",
    "turf.min.js"
  ],
  "scripts": {
    "pretest": "rollup -c ../../rollup.config.js",
    "test": "node test.js",
    "posttest": "node test.example.js",
    "build": "npm run pretest && browserify main.js -s turf -o turf.js",
    "postbuild": "uglifyjs turf.js -m -c -o turf.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Turfjs/turf.git"
  },
